script {
    use Assets::Store;
    use Std::Signer;

    fun read_write_loop(account: signer, iterations: u64) {
        while (iterations > 0) {
            iterations = iterations - 1;

            Store::store_u64(&account, iterations);
            let addr = Signer::address_of(&account);
            iterations = Store::load_u64(addr);
        }
    }
}

script {
    use Std::Vector;
    fun vector_loop(iterations: u64) {
        let vec = Vector::empty<u64>();
        while (iterations > 0) {
            iterations = iterations - 1;
            Vector::push_back(&mut vec, iterations);
           Vector::index_of(&vec, &iterations);
        }
    }
}


